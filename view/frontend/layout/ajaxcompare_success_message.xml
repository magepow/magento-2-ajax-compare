<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/layout_generic.xsd">
    <container name="root">
        <block class="Magento\Catalog\Block\Product\View" as="ajaxcompare_success_message"
               template="Magepow_AjaxCompare::success_message.phtml">
            <block class="Magepow\AjaxCompare\Block\Message" name="ajaxcompare.message" template="message.phtml"/>
            <block class="Magento\Catalog\Block\Product\Gallery" as="product_image" template="product/image.phtml"/>
        </block>
    </container>
</layout>
